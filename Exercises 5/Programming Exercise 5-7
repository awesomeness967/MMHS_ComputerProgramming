using System;
using static System.Console;
class MultiplicationTable
{
   static void Main()
   {
      Write("enter integer: ");
      int userInteger = int.Parse(ReadLine());
      int product;
      for (int i = 1; i <=10; i++) {
        product = i * userInteger;
        WriteLine(i + " x " + userInteger + " = " + product);
      }
   }
}
